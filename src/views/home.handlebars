<table id="productsTable" class="table" action="/api/products" method="get" > 
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Título</th>
            <th scope="col">Descripción</th>
            <th scope="col">Precio</th>
            <th scope="col">Thumbnail</th>
            <th scope="col">Código</th>
            <th scope="col">Stock</th>
            <th scope="col">Estado</th>
        </tr>
    </thead>
    <tbody>
        {{#each products}}
            {{#each}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.title}}</td>
            <td>{{this.description}}</td>
            <td>{{this.price}}</td>
            <td>{{this.thumbnail}}</td>
            <td>{{this.code}}</td>
            <td>{{this.stock}}</td>
            <td>{{this.status}}</td>
        </tr>
            {{/each}}
        {{/each}}
    </tbody>
</table>





<section>
<form id="addForm"  display="none">
</form><form id="removeForm"  display="none" >
</form>
</section>